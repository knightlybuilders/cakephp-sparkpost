{
    "name": "narendravaghela/cakephp-sparkpost",
    "description": "SparkPostEmail plugin for CakePHP",
    "license": "MIT",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "sparkpost", "email"],
    "homepage": "https://github.com/narendravaghela/cakephp-sparkpost",
    "authors": [
        {
            "name": "Narendra Vaghela",
            "homepage": "http://www.narendravaghela.com",
            "role": "Author"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.0.0 <4.0",
        "sparkpost/php-sparkpost": "^1.2"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "dev-master",
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "psr-4": {
            "SparkPostEmail\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SparkPostEmail\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
